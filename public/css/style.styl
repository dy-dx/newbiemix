@import 'nib'

[ng-cloak], .ng-cloak 
  display none

body
  // font 14px "Lucida Grande", Helvetica, Arial, sans-serif
  // text-shadow 0px 1px 0px hsla(0, 100%, 100%, 0.6)
  background-image url('/img/background.png')
  background-repeat no-repeat
  background-position center top

#container
  position relative
  margin 0 auto
  width 940px
  height 100%
  // text-align left
  // -webkit-perspective 1000
  -webkit-backface-visibility hidden

#loading 
  position fixed
  z-index 9999
  width 100%
  .alert
    position relative
    display inline
    padding 8px 14px
    left 470px
    margin-left -47px
#notification
  text-align center

.caret
  margin 13px 0 0 1px !important
.add-up
  position absolute
  left 50%
  button
    margin-top 10px !important
    margin-left -42px
    padding 11px 14px
    font-size 18px
    line-height normal
    border-radius 6px
  span
    font-size 12px
    margin 4px 0 0 -72px

/**
 * Navbar
 */
.classpicker-description
  text-align center
  position absolute
  top 64px
  width 340px
  span
    font-size 10px
  span:first-child
    position absolute
    left 0
    top 2px
  span:last-child
    position absolute
    right 0
    top 2px
.classpicker
  height 82px
  user-select none
.classpicker-icon
  width 64px
  height 64px
  margin 6px 2px
  text-align center
  color white
  font 12px "Lucida Grande", Helvetica, Arial, sans-serif
  text-shadow 1px 1px #000000
  border-radius 2px
.classpicker-icon-deselected
  opacity 0.4

#profile-dropdown
  margin-top 10px
  img
    border-radius 3px
.steamlogin
  margin-top 3px

/**
 * Pages
 */
.sidenav
  background-color rgba(255,255,255,0.88)
  border-radius 6px
  box-shadow 0 1px 4px rgba(0, 0, 0, .065)
  margin-bottom 20px
  li a
    border-top 1px solid #E5E5E5
    padding 8px 14px
    margin 0px -15px -1px -15px
  li:first-child a
    border-top none
    border-radius 6px 6px 0 0
  li:last-child a
    border-radius 0 0 6px 6px
.status
  min-height 20px
  // padding 8px 14px
  padding 16px 20px 16px 25px

.content
  // padding 39px 19px 14px
  padding 24px
  min-height 200px
  margin-bottom 20px
  background-color rgba(255,255,255,0.88)
  border 1px solid #E5E5E5
  // border 1px solid #E3E3E3
  // border 1px solid #DDD
  border-radius 6px
  box-shadow 0 1px 4px rgba(0, 0, 0, 0.065)

/**
 * Mix Overview
 */
.back-button
  position absolute
  top 14px
  left 18px
  width 50px
  text-align center
.back-button:hover
  filter brightness(0.1)
  -webkit-filter brightness(0.1)
  -moz-filter brightness(0.1)
  -o-filter brightness(0.1)
  -ms-filter brightness(0.1)
.back-button:hover:after
  content "Home"
.mix-teams
  margin 10px auto 40px
  width 800px
  text-align center
  ul
    background-color white
    border 2px solid #333
    border-radius 6px
    list-style none
    display inline-block
    vertical-align top
    text-align left
    margin 0 27px 0 0
  ul:last-child
    margin 0 0 0 27px
    li:first-child
      background-color #D06553
  li:first-child
    text-align center
    border-top 0
    border-radius 4px 4px 0 0
    font-size 24px
    line-height 32px
    color white
    background-color #79B3D2
  li
    width 350px
    position relative
    border-top 2px solid #333
    line-height 18px
  li a
    display block
    padding 5px 6px
  li a:hover
    background-color #EEE
  li:hover button
    display block
  li:last-child a
    border-radius 0 0 6px 6px
  img
    margin-top 2px
    border-radius 3px
    overflow hidden
  div
    display inline-block
  .label
    margin-bottom -2px

.class-icon
  position absolute
  right 2px
.name
  // display inline-block
  max-width 220px
  padding 0 5px
  white-space nowrap
  overflow hidden
  text-overflow ellipsis
.summary
  padding 0 5px
  font-size 12px
.report
  position absolute
  display none
  left 61%
  top 50%
  margin-top -20px

.connect-string
  input[readonly]
    width 360px
    background-color white
    outline none
    box-shadow none

.mix-lower
  width 100%
  text-align center
  margin 30px 0 0 0
.mix-info
  display inline-block
  text-align left
  dt
    width 80px //160px
  dd
    margin-left 100px //180px
  vertical-align top
  margin 20px 10px 0 0
  width 36%
.chatbox
  display inline-block
  width 60%
  margin 0 0 0 10px
  height 220px
  font normal 1em sans-serif
  // border 1px solid #ccc
  text-align left
  // padding 10px 15px 15px 15px 
  border-radius 5px
.chatlog
  height 70%
  overflow-x hidden
  overflow-y auto
  background-color white
  border none
  box-shadow inset 0 0 2px #777
  border-radius 5px
  margin 5px 0
  padding-top 6px
  padding-bottom 6px
  p
    font-size 0.9em
    color #000a68
    padding 3px 9px
    margin 0
.chat-time
  color #666
  font-size 0.8em
  padding-right 6px
.chat-input
  padding-right 10px
  input, input:focus
    width 100%
    padding 5px
    outline none
    border none
    -webkit-appearance none
    border-radius 5px
    box-shadow inset 0 0 2px #777

/**
 * Sprites
 */
.sprite
  position absolute
  // font-family '04b_03'
  .bubble
    position absolute
    left -15px
    width 294px - 20px - 21px
    max-height 133px - 21px - 42px
    border-width 21px 20px 42px 21px
    padding 5px
    display none
    // text-align center
    overflow-y auto
    white-space pre-wrap
    line-height 125%
    // &.frame-0
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
    // &.frame-1
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
    // &.frame-2
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
  ::-webkit-scrollbar
    width 1ex
  ::-webkit-scrollbar-thumb:vertical
    background-color hsla(0, 0%, 0%, 0.2)
  .textarea-container
    overflow hidden
    position relative
    textarea
      position absolute
      left -100px
      width 80px


.wysiwyg-text-align-left
  text-align left
.wysiwyg-text-align-center
  text-align center
.wysiwyg-text-align-right 
  text-align right