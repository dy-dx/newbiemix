[ng-cloak], .ng-cloak 
  display none

body
  // text-align center
  background hsl(200, 10%, 80%) url('/img/noise.png') repeat
  // font 14px "Lucida Grande", Helvetica, Arial, sans-serif
  // min-width 960px
  // text-shadow 0px 1px 0px hsla(0, 100%, 100%, 0.6)
  // overflow scroll

a
  // color #00B7FF

#container
  position relative
  // width 900px
  // margin 30px auto 0
  // text-align left
  // -webkit-perspective 1000
  -webkit-backface-visibility hidden

#loading 
  position fixed
  z-index 9999
  width 100%
  .alert
    position relative
    display inline
    padding 8px 14px
    left 50%
    margin-left -46px

.add-up
  position absolute
  left 50%
  margin-left -42px

.class-icon, .placeholder
  width 64px
  height 64px
  margin 10px 2px
  text-align center
  color white
  font 12px "Lucida Grande", Helvetica, Arial, sans-serif
.class-icon
  border 1px solid white
  background-color blue
.class-icon-deselected
  opacity 0.5


.home-sidenav
  // background-color white
  background-color whiteSmoke
  -webkit-border-radius 6px
  -moz-border-radius 6px
  border-radius 6px
  -webkit-box-shadow 0 1px 4px rgba(0, 0, 0, .065)
  -moz-box-shadow 0 1px 4px rgba(0, 0, 0, .065)
  box-shadow 0 1px 4px rgba(0, 0, 0, .065)
  li a
    border 1px solid #E5E5E5
    padding 8px 14px
    margin 0px -15px -1px -15px
  li:first-child a
    -webkit-border-radius 6px 6px 0 0
    -moz-border-radius 6px 6px 0 0
    border-radius 6px 6px 0 0
  li:last-child a
    -webkit-border-radius 0 0 6px 6px
    -moz-border-radius 0 0 6px 6px
    border-radius 0 0 6px 6px

.home-content
  // padding 39px 19px 14px
  padding 24px
  min-height 200px
  margin-bottom 20px
  // background-color white
  background-color whiteSmoke
  border 1px solid #E5E5E5
  // border 1px solid #E3E3E3
  // border 1px solid #DDD
  -webkit-border-radius 6px
  -moz-border-radius 6px
  border-radius 6px
  -webkit-box-shadow 0 1px 4px rgba(0, 0, 0, 0.065)
  -moz-box-shadow 0 1px 4px rgba(0, 0, 0, 0.065)
  box-shadow 0 1px 4px rgba(0, 0, 0, 0.065)


.mix-info
  background-color white
  text-align center
  ul
    list-style none
    display inline-block

.sprite
  position absolute
  // font-family '04b_03'
  .bubble
    position absolute
    left -15px
    width 294px - 20px - 21px
    max-height 133px - 21px - 42px
    border-width 21px 20px 42px 21px
    padding 5px
    display none
    // text-align center
    overflow-y auto
    white-space pre-wrap
    line-height 125%
    // &.frame-0
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
    // &.frame-1
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
    // &.frame-2
    //   border-image url('data:image/png;base64,') 21 20 42 21 fill
  ::-webkit-scrollbar
    width 1ex
  ::-webkit-scrollbar-thumb:vertical
    background-color hsla(0, 0%, 0%, 0.2)
  .textarea-container
    overflow hidden
    position relative
    textarea
      position absolute
      left -100px
      width 80px